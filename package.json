{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "prismlauncher",
  "title": "Prism Launcher",
  "description": "Launch Minecraft instances via Prism Launcher",
  "icon": "prismlauncher.svg",
  "author": "clrxbl",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Launch Minecraft Instance",
      "description": "Shows a list of available Prism Launcher instances",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.4",
    "@raycast/utils": "^1.17.0",
    "ini": "^4.1.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "22.5.1",
    "@types/react": "18.3.4",
    "eslint": "^9.9.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
